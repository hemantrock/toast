@using TOAST.Data.Model;
@model List<TOAST.Data.Model.tblConfigMaster>
@{
    ViewBag.Title = "Index";
    Layout = null;
}
<div class="row">
    <div class="col-lg-12 text-right">
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <h3 class="grid-header" style="color:black">VDU Parameter </h3>
        <table id="tblVDUParameter" class="table">
            <thead>
                <tr>
                    <th>Parameter Name</th>
                    <th>Parameter Description</th>
                    <th>Value</th>  
                </tr>
            </thead>
            <tbody>
                @foreach (tblConfigMaster itm in Model)
                {
                    <tr class="clickable">
                        <td>@itm.ParameterName</td>
                        <td>@itm.ParameterDesc</td>
                        <td>@itm.Value</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

</div>
<script src="~/Scripts/PageJS/Masters/VDUParameter/Index.js"></script>
