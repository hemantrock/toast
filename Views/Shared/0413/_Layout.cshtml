    <script>
        $(document).ready(function () {
            $('.svgToggle').click(function (e) {
                e.preventDefault();
                $('.svgButtons').toggleClass('on')
            })
            $(document).on("click", function (event) {
                let trigger = $(".togglerButton")[0],
                    dropdown = $(".togglerContent"),
                    svgButton = $('.svgButtons');
                if (dropdown !== event.target && !dropdown.has(event.target).length && trigger !== event.target) {
                    $(".togglerContent").hide();
                    $('#autoSearch').val('');
                }
                if (svgButton !== event.target && !svgButton.has(event.target).length && trigger !== event.target) {
                    $('.svgButtons').removeClass('on');
                }

            });
            $('.togglerButton').on('click', function (event) {
                event.stopPropagation();
                $('.togglerContent').slideToggle(350);
                $('#autoSearch').val('');
                $('#autoSearch').focus();
            });
            $('#autoSearch').on('keydown', function (event) {
                if (event.keyCode === 27) {
                    $(this).val('');
                    $('.togglerContent').slideToggle(350);
                }
            })
            $("#autoSearch").autocomplete({
                source: @Html.Raw(Json.Encode(lstMnu)),
                minLength: 2,
                autoFocus: false,
                focus: function (event, ui) {
                    //event.preventDefault();
                    $('#autoSearch').val(ui.item.label);
                    event.preventDefault();
                },

                select: function (event, ui) {
                    event.preventDefault();
                    $('#autoSearch').val(ui.item.label);
                    clearControlsSubscriptions();
                    $(document).click(eval(ui.item.action));
                    $('.togglerContent').slideToggle(350);
                    event.target.value = '';
                    return false;
                }
            });
            //$(".fa-bell").click(function () {
            //    $('.msg-alert').toggle(
            //        function () {
            //        $(this).animate({
            //            'height': 'auto',
