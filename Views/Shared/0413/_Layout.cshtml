@using TOAST.Business;
@using TOAST.Data.Model;
@using TOAST.Shared;
@{
    string FuncName = "OpenDashboardView";
    string strParams = "";
    List<MenuSearch> lstMnu = new List<MenuSearch>();
    List<tblMenuConfig> lstShortcuts = new List<tblMenuConfig>();
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="X-UA-Compatible" content="IE=10" />
    <meta http-equiv="PRAGMA" content="NO-CACHE">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>TOAST</title>
    <link href="~/Content/images/favicon.ico" rel="shortcut icon">
    @Styles.Render("~/Content/bundlecss")
    @Scripts.Render("~/bundles/javascripts")
    <link href="~/fonts/font-awesome-5.12.0/css/all.min.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <script src="~/Scripts/Script.js"></script>
    <script src="~/signalr/hubs"></script>
    <script type="text/javascript">
        var availableTags;
        var opc = $.connection.opcServer;

        function LRC_Changed(ActiveLRC) {
            console.log("LRC changed to: " + ActiveLRC);
            window.location.href = "/Home/GoToHome";
            setTimeout(function () { alertDismissable("LRC has changed. Redirecting to " + ActiveLRC + ".....") }, 2000);
            //window.location.href = "/Home/ClearUser";
        }

        opc.client.updateViewData = function (model, grpName) {
            ShowOPCTagsinView(model, grpName);
        }

        opc.client.showAlertMsg = function (result) {
            ShowAlertMsg(result);
        }

        opc.client.validateTanksModePermission = function (TankUnqId, TagName, UserID, result) {
            ConfirmTankModeChange(TankUnqId, TagName, UserID, result);
        }

        opc.client.validateTankAutoManualPermission = function (TankUnqId, TagName, UserID, result) {
            ConfirmTankAutoManualChange(TankUnqId, TagName, UserID, result);
        }

        opc.client.callJSFunction = function (funcName, param1, param2, param3, param4, param5) {
            CallJSFunction(funcName, param1, param2, param3, param4, param5);
        }

        opc.client.callobjJSFunction = function (remScrObj) {
            CallobjJSFunction(remScrObj);
        }

        $.connection.hub.start()
            .done(function () {
                opcStartDone = true;
                console.log(showTime() + ": Connected, transport = " + $.connection.hub.transport.name);
                opc.server.readPageTags("1ST");
            });

        $.connection.hub.connectionSlow(function () {
            alertDismissable("danger", "Connection Slow.");
        });

        var tryingToReconnect = false;

        $.connection.hub.reconnecting(function () {
            tryingToReconnect = true;
            alertDismissable("danger", "Trying To Reconnect.");
        });

        $.connection.hub.reconnected(function () {
            tryingToReconnect = false;
            alertDismissable("danger", "Sever Reconnected.");
        });

        $.connection.hub.disconnected(function () {
            //After the timeout  defined in Startup.cs, hub.error will be thrown and then disconnection will happen.
            //alertDismissable("danger", "DisConnected Re-directing to Backup Server:" + strBackupSrv);
            console.log('SignalR Disconnected...');
            CheckReDirectToBackup();
        });

        $.connection.hub.error(function (error) {
            console.log('SignalR error: ' + error); // After timeout this error is first generated and then disconnection happens.
            if (error.message.indexOf("closed") > 0) { window.location.href = "/Home/GoToHome"; }
        });
    </script>
</head>

<body>
    <aside class="sidebar">
        <div class="toggle">
            <button type="button" class="navbar-toggle burger js-menu-toggle" data-toggle="collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div class="sidebar-row">
            <div class="userDetail">
                <div class="client-logo @(clsConfig.CustomerName)"></div>
                <div class="text-center icon-control-text">@Html.Raw(clsConfig.SolName)</div>
            </div>
            <div class="navbar-collapse pad-0">
                <ul class="nav navbar-nav">
                    @{
                        SharedBL objBL = new SharedBL(Convert.ToInt32(Session[clsConstants.UserID]));
                        List<tblMenuConfig> lst = objBL.GetMenuList(Convert.ToBoolean(Session[clsConstants.IsMobile]), ref FuncName, ref strParams, ref lstShortcuts);

                        if (lst == null)
                        {
                            Response.Redirect("/Home/Index");
                        }
                        else
                        {
                            foreach (tblMenuConfig menu in lst.Where(x => x.MainMenuId == null))
                            {
                                if (lst.Where(x => x.MainMenuId == menu.UniqueID).Count() <= 0) { continue; }
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle slideLI" data-level="1" data-toggle="dropdown" title="@menu.ToolTip">@menu.MenuTitle<span class="caret"></span></a>
                                    <ul class="dropdown-menu" role="menu">
                                        @foreach (tblMenuConfig subMenu in lst.Where(x => x.MainMenuId == menu.UniqueID))
                                        {
                                            <li id="submenu-@subMenu.UniqueID">
                                                @if (lst.Where(x => x.MainMenuId == subMenu.UniqueID).ToList().Count() > 0)
                                                {
                                                    <a title="@subMenu.ToolTip" class="dropdown-toggle slideLI" data-level="2" data-toggle="dropdown">
                                                        <i class="@subMenu.glyphicon"></i>
                                                        <span class="caret"></span>
                                                        @subMenu.MenuTitle
                                                    </a>
                                                }
                                                else
                                                {
                                                    <a onclick='@subMenu.JSFunction' title="@subMenu.ToolTip" class="dropdown-toggle" data-toggle="dropdown">
                                                        <i class="@subMenu.glyphicon"></i>
                                                        <span class="caret"></span>
                                                        @subMenu.MenuTitle
                                                    </a>
                                                }
                                                @if (lst.Where(x => x.MainMenuId == subMenu.UniqueID).ToList().Count() > 0 && subMenu.MenuClientId != "mnuBayView_HMI")
                                                {
                                                    <ul class="dropdown-submenu" role="menu">

                                                        @foreach (tblMenuConfig subSubMenu in lst.Where(x => x.MainMenuId == subMenu.UniqueID))
                                                        {
                                                            <li><a herf="#" onclick='@subSubMenu.JSFunction'><i class="@subSubMenu.glyphicon"></i> @subSubMenu.MenuTitle</a></li>
                                                            if (!String.IsNullOrWhiteSpace(subSubMenu.JSFunction))
                                                            {
                                                                lstMnu.Add(new MenuSearch() { label = subSubMenu.MenuTitle, value = subSubMenu.UniqueID, action = subSubMenu.JSFunction });
                                                            }
                                                        }
                                                    </ul>
                                                }
                                                @if (subMenu.MenuClientId == "mnuBayView_HMI")
                                                {
                                                    <ul class="dropdown-submenu" role="menu">
                                                        @foreach (tblBaysMaster oBay in clsConfig.BayViewList)
                                                        {
                                                            <li><a herf="#" onclick="OpenBayViewHMIBasedOnSelectedBay('@oBay.BayID')"><i class="fa fa-street-view"></i>@oBay.BayName</a></li>
                                                            lstMnu.Add(new MenuSearch() { label = oBay.BayName, value = (int)oBay.BayID, action = "OpenBayViewHMIBasedOnSelectedBay(" + oBay.BayID + ")" });
                                                        }
                                                    </ul>
                                                }
                                                @if (!String.IsNullOrWhiteSpace(subMenu.JSFunction))
                                                {
                                                    lstMnu.Add(new MenuSearch() { label = subMenu.MenuTitle, value = subMenu.UniqueID, action = subMenu.JSFunction });
                                                }
                                            </li>
                                        }
                                    </ul>
                                </li>
                            }
                        }
                    }
                </ul>

            </div>
            <div class="menu-short">
                <ul class="">
                    @{
                        foreach (tblMenuConfig mnu in lstShortcuts)
                        {
                            <li class="">
                                <a onclick='@mnu.JSFunction' title="@mnu.MenuTitle" class="tooltips">
                                    <i class="@mnu.glyphicon"></i>
                                    Load Summary
                                </a>
                            </li>
                        }
                    }
                </ul>
            </div>
            <div class="menu-action">
                <ul>
                    <li class="relative">
                        <div class="svgButtons ui-widget-content">
                            <div class="slideButtons">
                                <a class="button top" href="javascript:void(0);" onclick="panZoomInstance.fit(); panZoomInstance.pan({ x: 0, y: 0 });"><i class="fas fa-expand" title="Fit"></i></a>
                                <a class="button right " href="javascript:void(0);" onclick="panZoomInstance.reset();"><i class="fas fa-undo" title="Reset"></i></a>
                                <a class="button left btnPan" href="javascript:void(0);" onclick="SetPan(this); cancelBubble(event);">
                                    <i class="fas fa-hand-paper" title="Pan"></i>
                                </a>
                                <a class="button bottom btnZoom" href="javascript:void(0);" onclick="SetZoom(this); cancelBubble(event);"><i class="fas fa-search-plus" title="Zoom"></i></a>
                            </div>
                            <div class="center-button svgToggle" href="javascript:void(0);" title="Move Button and Expand on click"><i class="fas fa-plus" title=""></i></div>
                        </div>
                    </li>
                    <li class="relative">
                        <span class="togglerButton site-search-btn"><i class="fas fa-search"></i></span>
                        <div class="togglerContent searchBox">
                            <span class="up-arrow"></span>
                            <label><i class="fas fa-search"></i><input id="autoSearch" type="text" name="search" /></label>
                        </div>
                    </li>
                    <!--<li class="dashboard" title="Home"><span class="home glyphicon glyphicon-home" onclick="clearControlsSubscriptions(); OpenDashboardView('@Session[clsConstants.UserID]');" aria-hidden="true"></span></li>
                    <li class="legend" title="Legends" onclick="OpenLegendsViewHMI()"><i class="fa fa-info"></i></li>-->
                    <li class="settings" title="Settings"> <span aria-hidden="true" title="Settings" class="setting glyphicon glyphicon-cog" onclick="ShowUserSettings();"></span></li>
                    <li class="relative">
                        <!--User Profile section-->
                        <div class="logout" id="Logout"><span class="user">@Convert.ToString(System.Web.HttpContext.Current.Session[clsConstants.UserName])</span><span id="userIcon" class="icon glyphicon glyphicon-user"></span></div>
                        <div id="UserDetail">
                            <span class="up-arrow"></span>
                            <ul>
                                <li>Login ID: <span class="userRole">@Convert.ToString(System.Web.HttpContext.Current.Session[clsConstants.LoginID])</span></li>
                                <li>User Name: <span class="userRole">@Convert.ToString(System.Web.HttpContext.Current.Session[clsConstants.UserName])</span></li>
                                <li>Master Server: <span class="userRole">@HttpContext.Current.ApplicationInstance.Server.MachineName</span></li>
                                @*<li>Login DateTime: <span class="userRole">@HttpContext.Current.Session[clsConstants.LoginDateTime]</span></li>*@
                                <li>Machine ID: <span class="userRole">@(HttpContext.Current.Session[clsConstants.MachineAddress])</span></li>
                                <li>User Role: <span class="userRole">@Convert.ToString(System.Web.HttpContext.Current.Session[clsConstants.UserRole])</span></li>

                            </ul>

                            <!--<div class="role-bar">
                                <a class="btn btn-primary" href="#" role="button" style="float:left;" onclick="ShowUserSettings();">Settings</a>
                            </div>-->

                            <div class="logoutBtnBar">
                                <a class="btn btn-primary" href="#" role="button" style="float:left;" onclick="ChangePassword(@Convert.ToString(Session[clsConstants.UserID]));">Change Password</a>
                                <a class="btn btn-primary" href="#" role="button" onclick="LogoutUser(false);">Logout</a>
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </aside>
    <main>
