    <script>
        $(function () {
            function closemenu() {
                $('body').removeClass('show-sidebar');
                $('.js-menu-toggle').removeClass('active');
                $('.navbar-nav').css({ transform: 'translateX(0)' }).find('.activeOn.active').removeClass('activeOn active');
            }
            $('.js-menu-toggle').click(function (e) {
                var $this = $(this);
                if ($('body').hasClass('show-sidebar')) {
                    closemenu();
                } else {
                    $('body').addClass('show-sidebar');
                    $this.addClass('active');
                }
                e.preventDefault();
            });
            // click outisde offcanvas
            $(document).mouseup(function (e) {
                var container = $(".sidebar");
                if (!container.is(e.target) && container.has(e.target).length === 0) {
                    if ($('body').hasClass('show-sidebar')) {
                        $('body').removeClass('show-sidebar');
                        $('body').find('.js-menu-toggle').removeClass('active');
                        $('.navbar-nav').css({ transform: 'translateX(0)' }).find('li').removeClass('active activeOn');
                    }
                }
            });
            $('.navbar-nav >li >a.slideLI').each(function () {
                let backBtn = (no, text) => '<li class="text-right back-1" data-level="' + no + '"><a href="javascript:void();" class="noanchor"><i class="fas fa-chevron-left"></i> ' + text + '</span></li>';
                let _text = $(this).text();
                $(this).next('ul').prepend(backBtn(0, _text));
                $(this).next('ul').find('a.slideLI').each(function () {
                    let _text = $(this).text();
                    $(this).next('ul').prepend(backBtn(1, _text));
                })
            })
            $('.slideLI').click(function () {
                let level = $(this).data('level'),
                    translate = level * 100;
                $(this).parent('li').addClass('activeOn');
                $('.sidebar-row').scrollTop(0)
                $('.navbar-nav').css({ transform: 'translateX(-' + translate + '%)' })
            });
            $(document).on('click', '.back-1', function (e) {
                e.preventDefault();
                let level = $(this).data('level'),
                    translate = level * 100;
                $('.navbar-nav').css({ transform: 'translateX(-' + translate + '%)' });
                let _this = $(this);
                $(this).closest('.activeOn').removeClass('active activeOn');
            });
            $('.navbar-nav >li a').click(function () {
                if (!$(this).hasClass('slideLI') && !$(this).hasClass('noanchor')) {
                    closemenu();
                }
            })

        });
        function passHideShow(e) {
            var x = document.getElementById("Pwd_txtPassword");
            if (x.type === "password") {
                x.type = "text";
                $(e).removeClass('glyphicon-eye-close').addClass('glyphicon-eye-open');
            } else {
                x.type = "password";
                $(e).addClass('glyphicon-eye-close').removeClass('glyphicon-eye-open');
            }
        }
    </script>

