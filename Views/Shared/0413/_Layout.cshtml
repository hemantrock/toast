            // configure for module loader

            require.config({
                paths: {
                    echarts: '../../../../Scripts'
                }
            });
            SetDefaultRows();

            if ($(".modal-dialog").hasClass('chartModalDialog') == false) {
                $(".modal-dialog").css('top', (($(window).height() - 400) / 2));
            }
            else {
                $(".modal-dialog").css('top', (($(window).height() - 500) / 2));
            }
            $('.dropdown-submenu').parent().addClass('dropsubmenu');
            $('.dropdown-submenu').hover(function () {
                $(this).parent().addClass('hover');
            }, function () {
                $(this).parent().removeClass('hover');
            })

            //var strPrimarySrv;
            //var strBackupSrv;

            $.ajax({
                url: "/Home/GetServers",
                type: "GET",
                datatype: "json",
                cache: false,
                success: function (result) {
                    $.each(result, function (i, obj) {
                        if (i == 0) {
                            strPrimarySrv = obj.DeviceName;
                        }
                        else if (i == 1) {
                            strBackupSrv = obj.DeviceName;
                        }
                    });

                    var strHost = window.location.host;  //window.location.hostname
                    if (strHost.toLowerCase().replace('/', '') != strPrimarySrv.toLowerCase().replace('/', '')) {
                        strBackupSrv = strPrimarySrv;
                        strPrimarySrv = strHost;
                    }
                    $.ajaxSetup({
                        global: true,
                        timeout: 60000,
                        error: function (jqXHR, textStatus, errorThrown) {
                            processAjaxError(jqXHR, textStatus, errorThrown);
                        },
                    });
                }
            });

            GetStatusBarAck();

            $(document).ajaxStart(function () {
                onBeginAjax();
            });
            $(document).ajaxStop(function () {
                //if ($('#dvLoadSummary').length <= 0 && $('#dvTimer').length > 0) {
                //    $('#dvTimer').timer('pause');
                //}
                onCompleteAjax();
            });

            $('.navbar-nav li').on('click', function () {
                $('.navbar-nav li').removeClass('active');
                $(this).addClass('active');
            });

            $('ul.dropdown-menu li a').on('click', function () {
                clearControlsSubscriptions();
            });
            $('.menu-short ul li a').on('click', function () {
                clearControlsSubscriptions();
            });

            window['@FuncName'](@Html.Raw(strParams));

            $('#chartModal').on('hidden.bs.modal', function () {
                $('.chartModal-chart').hide();
            })

            $(function () {
                setInterval(function () { CallGlobalRefreshFunctions(); }, 40000);
            });

            $('#dtpChartFrom').datetimepicker({
                format: 'D-MMM-YY HH:mm',
                //debug: true
            });

            $('#dtpChartTo').datetimepicker({
                format: 'D-MMM-YY HH:mm',
                //debug: true
            });

            createTooltip();

            // Need to remove after some time
            setTimeout(function () {

                //removeMsgAlertItem($('[data-attr="msg-1"]'), false);
            }, 5000);

            if ($('.msg-alert ul li').length > 0) {
                $('.alert-action .msg-alert-bell').addClass('has-alert-item');
            }

            $('.msg-alert-bell').click(function () {
                showHideMsgAlert();
            });
            $('.close-msg-alert').click(function () {
                showHideMsgAlert();
            });

            $('.remove-msg-alert-icon').click(function () {
                removeMsgAlertItem($(this), true);
                if ($('.msg-alert ul li').length < 1) {
                    $('.alert-action .msg-alert-bell').removeClass('has-alert-item');
                }
            });

            //Set Config Values in JS
            ERPName = '@clsConfig.ERPName';
            ProjNo = '@clsConfig.ProjPath';
            TFMSLevelUnit = '@clsConfig.TFMSLevelUnit';
            gblApplication = '@clsConfig.Application';
            gblCustName = '@clsConfig.CustomerName';
            gblProtocol = '@clsConfig.SAPProtocol';
