@using TOAST.Data.Model;
@{

}
<div class="row">
    <div class="col-lg-2">
        <h3 class="grid-headerplant" style="color:black">Day Start End</h3>
    </div>
    <div class="col-lg-10 text-right">
        <div class="col-lg-6">
            
        </div>
        <div class="col-lg-6 ">
            <input id="dtpDayStartDate" name="DayStartDate" type="text" placeholder="Day Start Date" class="form-control" value="@DateTime.Now" style="width: 235px; margin-right: 10px; display: inline-block;">
            <a id="DayStartEndFilter" role="button" class="btn btn-primary" href="#" onclick="DayStartEndFilter();"><span class="glyphicon glyphicon-search"></span>Filter</a>
            <a role="button" class="btn btn-primary @((ViewBag.DayStart??false)?"":"hide")" id="btnDayStart" href="#" onclick="AddDayStartEnd('DAYSTART')"><i class="fa  fas fa-hourglass-start" aria-hidden="true"></i> Day Start</a>
            <a role="button" class="btn btn-primary @((ViewBag.DayEnd??false)?"":"hide")" id="btnDayEnd" href="#" onclick="AddDayStartEnd('DAYEND')"><i class="fa fas fa-hourglass-end" aria-hidden="true"></i> Day End</a>
            <a role="button" class="btn btn-primary @((ViewBag.IsDeviation??false)?"":"hide")" href="#" id="btnAckDeviation" onclick="AckDeviations()"><i class="fa fas fa-check" aria-hidden="true"></i>Ack Deviation</a>
        </div>
         
        
    </div>
</div>

<div class="tab-wrapper w-100 m-t-20">
    <div class="panel with-nav-tabs panel-default">
        <div class="panel-heading">
            <ul class="nav nav-tabs">
                    <li class="active"><a href="#Tanks" data-toggle="tab">Tanks Data</a></li>
                    <li><a href="#Meters" data-toggle="tab">Meters Data</a></li>
            </ul>
        </div>
        <div class="panel-body">
            <div class="tab-content">
                <div class="tab-pane active" id="Tanks">
                    <div id="jqxgridTFMS" class="jqx-tbl-container">
                    </div>
                </div>
                <div class="tab-pane fade" id="Meters">
                    <div id="jqxgridMeters" class="jqx-tbl-container">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/PageJS/Transactions/DayStartEnd/0409/Index.js"></script>


