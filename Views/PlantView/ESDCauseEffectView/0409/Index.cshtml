                        <td><i class="fa HTR_HCD_STATUS" aria-hidden="true"></i></td>
                        <td><i class="fa HTR_HCD_STATUS" aria-hidden="true"></i></td>
                        <td><i class="fa X_HTR_FAP_DYK_STATUS" aria-hidden="true"></i></td>
                        <td><i class="fa X_ALL_BG_OPENED" aria-hidden="true"></i></td>
                        <td><i class="fa X_ALL_TANKS_IN_DORMANT" aria-hidden="true"></i></td>
