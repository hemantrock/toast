@using TOAST.Data.Model;
@model TOAST.Data.Model.tblWLoadRakeDetail
@{
    ViewBag.Title = "Wagon Cancel";
    Layout = null;
}
<div class="col-lg-12 col-md-12 form-container">
    <form id="FormBU" role="form" class="form-fields">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="fields form-group">
                    <label>Rake Numbner-SeqNo</label>
                    <input type="text" id="txtRakeDetails" name="WagonNumber" maxlength="50" value="@(Model.tblWagonMaster.WagonNo)-@(Model.WagonSeqNo)" class="form-control" disabled />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="fields form-group">
                    <label>Reason</label>
                    <select id="ddlManualEntry" name="Reason" class="form-control"></select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="fields form-group">
                    <label>Remarks</label>
                    <input type="text" id="txtRemarks" name="Remarks" autocomplete="off" maxlength="50" class="form-control" />
                </div>
            </div>
        </div>
    </form>
</div>
<script>
</script>
<script src="~/Scripts/PageJS/Wagon/Summary/WagonCancel.js"></script>